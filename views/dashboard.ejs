	google.charts.load('current', {'packages':['gauge']});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {

        var data = google.visualization.arrayToDataTable([
          ['Label', 'Value'],
          ['Quality', 20]
        ]);

        var options = {
          width: 200, height: 220,
          redFrom: 0, redTo: 25,
          yellowFrom:25, yellowTo: 75,
          greenFrom: 75, greenTo: 100,
          minorTicks: 5
        };

        var chart = new google.visualization.Gauge(document.getElementById('chart_div'));

        chart.draw(data, options);

        setInterval(function() {
          data.setValue(0, 1, 40 + Math.round(60 * Math.random()));
          chart.draw(data, options);
        }, 13000);
        setInterval(function() {
          data.setValue(1, 1, 40 + Math.round(60 * Math.random()));
          chart.draw(data, options);
        }, 5000);
        setInterval(function() {
          data.setValue(2, 1, 60 + Math.round(20 * Math.random()));
          chart.draw(data, options);
        }, 26000);
    }

</script>

<script>
      function initMap() {

      	var myCity = $("#startCity").val();
		var myState = $("#startState").val()
		var myLat = parseFloat($("#latitude").val())
		var myLong = parseFloat($("#longitude").val())
		console.log("CITYSTATE");
		console.log(myCity);
		console.log(myState);
		console.log(myLat);
		console.log(myLong);

        var uluru = {lat: myLong, lng: myLat};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 10,
          center: uluru
        });
        var infowindow = null;
		/* now inside your initialise function */
		infowindow = new google.maps.InfoWindow({
		content: "holding..."
		});

		
		

        $.ajax({
		  method: "POST",
		  url: "/ajax/stores",
		  data: { latitude: myLong, longitude: myLat }
		})
		  .done(function( msg ) 
		  {
		  	console.log(msg);
		  	for (x in msg)
		  	{
		  		var latLng = new google.maps.LatLng(msg[x].coordinate.latitude,msg[x].coordinate.longitude);
		          var marker = new google.maps.Marker({
		            position: latLng,
		            map: map,
		            title: msg[x].name,
		            info: msg[x].name,
		            html: "<h3>"+msg[x].name+"</h3>"
		    	})		// console.log(msg)
		  	
			  	google.maps.event.addListener(marker, 'click', function () {
				// where I have added .html to the marker object.
				infowindow.setContent(this.html);
				infowindow.open(map, this);
				});
			}
		});
    }


     
    </script>
